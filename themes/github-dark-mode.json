        "foreground": "#f0f6fc"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "background": "#8e1519",
        "foreground": "#f0f6fc"
      }
    },
    {
      "scope": "invalid.illegal",
