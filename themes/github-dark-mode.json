  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "variable.object.property",
        "variable.other.object.property",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "constant.language.import-export-all"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
