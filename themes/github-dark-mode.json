      }
    },
    {
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "keyword.operator.type",
        "keyword.operator.optional"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
