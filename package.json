{
  "name": "github-dark-mode",
  "version": "0.0.1",
  "description": "Github Dark Mode for Visual Studio Code",
  "license": "MIT",
  "engines": {
    "vscode": "^1.52.0"
  },
  "scripts": {
    "dev": "concurrently -k \"tsc --watch\" \"nodemon -w ./dist ./dist/index.js\"",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/lodash.flow": "^3.5.6",
    "@types/node": "^14.14.12",
    "concurrently": "^5.3.0",
    "lodash.flow": "^3.5.0",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.2"
  },
  "contributes": {
    "themes": [
      {
        "label": "GitHub Dark Mode",
        "uiTheme": "vs-dark",
        "path": "./themes/github-dark-mode.json"
      }
    ]
  }
}
